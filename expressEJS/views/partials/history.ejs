<div class="table-container">
    <h1>Historial</h1>
    <div class="table">
        <%if(users.length>0){%>
            <div class="row">
                <div class="col">
                    NOMBRE
                </div>
                <div class="col">
                    APELLIDO
                </div>
                <div class="col">
                    EDAD
                </div>
            </div>
            <% users.forEach(function(user){%>
                <div class="row">
                    <div class="col">
                        <%=user.name%>
                    </div>
                    <div class="col">
                        <%=user.last_name%>
                    </div>
                    <div class="col">
                        <%=user.age%>
                    </div>
                </div>
            <%})%>
        <%}else{%>
            <p>No hay ning√∫n usuario registrado</p>
        <%}%>
    </div>
</div>